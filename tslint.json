{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-ordering": [
      true
    ],
    "interface-name": [
      false
    ],
    "semicolon": [
      true,
      "never"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "jsdoc-format": true,
    "member-access": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "trailing-comma": [
      false,
      {
        "multiline": {
          "objects": "false",
          "arrays": "false",
          "functions": "never",
          "typeLiterals": "ignore"
        },
      }
    ],
    "no-console": [
      true,
      "debug",
      "log",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-empty-interface": true,
    "no-for-in-array": true,
    "no-import-side-effect": [
      true,
      {
        "ignore-pattern": "/rxjs.*/"
      }
    ],
    "no-inferrable-types": [
      true,
      "ignore-params"
    ],
    "no-internal-module": true,
    "no-invalid-template-strings": true,
    "no-irregular-whitespace": true,
    "no-shadowed-variable": [
      true,
      {
        "temporalDeadZone": false
      }
    ],
    "no-string-literal": false,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unnecessary-initializer": true,
    "no-unused-expression": true,
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_$"
      }
    ],
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-finally",
      "check-open-brace",
      "check-whitespace"
    ],
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "prefer-const": true,
    "prefer-for-of": true,
    "prefer-object-spread": true,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "asyncArrow": "always",
        "constructor": "never",
        "method": "never",
        "named": "never"
      }
    ],
    "triple-equals": [
      true,
      "allow-undefined-check"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-preblock",
      "check-separator",
      "check-type"
    ]
  },
  "rulesDirectory": []
}